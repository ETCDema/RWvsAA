	"configurations": [
		{
			"name": "Static WEB on IISExpress",
			"type":"coreclr",
			"request": "launch",
			"program": "C:\\Program Files\\IIS Express\\iisexpress.exe",
			"args": ["/path:${workspaceFolder}", "/port:9090"],
			"cwd": "${workspaceFolder}",
			"stopAtEntry": false
		}
	]
}